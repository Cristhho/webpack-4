{
  "name": "platzi-webpack",
  "version": "1.0.0",
  "description": "Curso de webpack en Platzi",
  "main": "index.js",
  "scripts": {
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugin": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js",
    "build:prepro": "webpack-dev-server --config ./prepro/webpack.config.js",
    "build:prevent-dupli": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:prevent-dupli-fast": "webpack --config ./dll/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Cristhho/webpack-4.git"
  },
  "keywords": [
    "webpack",
    "platzi"
  ],
  "author": "@cristhhq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cristhho/webpack-4/issues"
  },
  "homepage": "https://github.com/Cristhho/webpack-4#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
